#header= render_partial 'header', :title => $resume.data["Title"]
#main.tab-content
  .tab-pane.active#summary
    = render_partial 'summary', :contact => $resume.data["Contact Info"], :summary => $resume.data["Summary"]
  .tab-pane#skills
    = render_partial 'skills', :skills => $resume.data["Skills"]
  .tab-pane#education
    = render_partial 'education', :education => $resume.data["Education"]
  .tab-pane#experience
    = render_partial 'experience', :experience => $resume.data["Experience"]
  .tab-pane#contacts
    = render_partial 'contacts'
#footer= render_partial 'footer'

